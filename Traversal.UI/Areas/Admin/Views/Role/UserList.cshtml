@model List<AppUser>
@{
    ViewData["Title"] = "UserList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Assing Role</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td></td>
            <td><a href="/Admin/Role/AssingRole/@item.Id" class="btn btn-warning"> Assing Role </a></td>
        </tr>
    }
</table>
